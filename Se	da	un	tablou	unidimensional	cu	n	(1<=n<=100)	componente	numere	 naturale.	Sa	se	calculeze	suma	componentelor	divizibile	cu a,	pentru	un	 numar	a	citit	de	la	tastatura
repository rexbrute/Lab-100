#include <stdio.h>
#include <stdlib.h>

int main()
{
  int tab[100];
  int n,a,i,s=0;
  scanf("%d",&n);
   for(i=0; i<n; i++)
    {
 scanf("%d",&tab[i]);
    }
    printf("Suma numerelor divizibile cu ...");
    scanf("%d",&a);
    for(i=0;i<n;i++)
    {
        if(tab[i]%a==0)s=tab[i]+s;

    }
    printf("Suma nr div cu %d este %d :", a,s);
    return 0;
}
