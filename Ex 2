#include <stdio.h>
#include <stdlib.h>

int main()
{
   int x[100];
   int i,a,b,n,s,num;
   int media;


    printf("Introduceti marime tablou: ");
    scanf("%d",&n);
    for(i=0; i<n; i++){
        scanf("%d",&x[i]);
}
printf("Introduceti a&b: ");
    scanf("%d %d",&a, &b);

for(i=0;i<n;i++){
if(a<=x[i]&&x[i]<=b)
{s=s+x[i];
num++;}
}

printf("Media este:%f", (float) s/num);

    return 0;
}

